# NL2SQL Generator - Frontend Environment Variables
# Copy this file to .env and update with your values

# Backend API URL
VITE_API_BASE_URL=http://localhost:8000/api/v1

# App Configuration
VITE_APP_NAME=NL2SQL Generator
VITE_APP_VERSION=1.0.0

# Feature Flags (for phased implementation)
VITE_ENABLE_VISUAL_EDITOR=false  # Enable in Phase 2
VITE_ENABLE_AI_MODIFICATION=false  # Enable in Phase 2
VITE_ENABLE_DATABASE_EXECUTION=false  # Enable in Phase 3
VITE_ENABLE_EXPORT=false  # Enable in Phase 4

# Diagram Settings
VITE_DEFAULT_DIAGRAM_FORMAT=mermaid  # mermaid or plantuml
